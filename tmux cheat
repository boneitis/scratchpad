tmux new -s <session-name>
"    attach -t <session-name>
"    ls
"    kill-session -t <session-name>

c - create window
, - rename window
n - next
p - prev
w - ls windows
t - show time
d - detach
[ - scrollback

% - split
" - split
o - next pane
q - ls panes
z - maximize pane
alt + arrow - resize pane
